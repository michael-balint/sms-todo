{
  "name": "respond",
  "runtime": "nodejs4.3",
  "description": "Serverless Lambda function for project: sms-todo",
  "customName": false,
  "customRole": false,
  "handler": "handler.handler",
  "timeout": 6,
  "memorySize": 1024,
  "authorizer": {},
  "custom": {
    "excludePatterns": []
  },
  "endpoints": [
    {
      "path": "respond",
      "method": "GET",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": false,
      "requestParameters": {
        "integration.request.querystring.To": "method.request.querystring.To",
        "integration.request.querystring.From": "method.request.querystring.From",
        "integration.request.querystring.Text": "method.request.querystring.Text",
        "integration.request.querystring.Type": "method.request.querystring.Type",
        "integration.request.querystring.MessageUUID": "method.request.querystring.MessageUUID"
      },
      "requestTemplates": {
        "application/json": "{\n\"To\": \"$input.params('To')\",\n\"From\": \"$input.params('From')\",\n\"Text\": \"$input.params('Text')\",\n\"MessageUUID\": \"$input.params('MessageUUID')\"}"
      },
      "responses": {
        "400": {
          "statusCode": "400"
        },
        "default": {
          "statusCode": "200",
          "responseParameters": {},
          "responseModels": {
            "application/json;charset=UTF-8": "Empty"
          },
          "responseTemplates": {
            "application/json;charset=UTF-8": ""
          }
        }
      }
    }
  ],
  "events": [],
  "environment": {
    "SERVERLESS_PROJECT": "${project}",
    "SERVERLESS_STAGE": "${stage}",
    "SERVERLESS_REGION": "${region}"
  },
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}
